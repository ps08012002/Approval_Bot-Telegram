generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tb_cabang {
  id         Int        @id @default(autoincrement())
  cabang String     @db.VarChar(50)
}

model tb_report {
  id              Int     @id @default(autoincrement())
  tanggal         Int     @default(0) 
  nama            String  @db.Text
  cabang          String  @db.VarChar(50)
  barang          String  @db.VarChar(50) 
  quantity        Int     @default(0)
  status          String  @db.VarChar(50) @default("pending")
  approveby       String  @db.VarChar(50) @default("-----")
}